  <div class="container">
    <div class="row">
      <!-- Social media icons/links -->
      <div class="col s12">
        <ul class="social-media-row">
          <li>
            <%= link_to 'https://www.facebook.com/Studentorkesterfestivalen', :class => 'btn-flat btn-social-media waves-effect waves-light', target: :'_blank' do %>
                <i class="icon-facebook socialmedia-icon"></i>
            <% end %>
          </li>
          <li>
            <%= link_to 'https://www.instagram.com/studentorkesterfestivalen/', :class => 'btn-flat btn-social-media waves-effect waves-light', target: :'_blank' do %>
                <i class="icon-instagram socialmedia-icon"></i>
            <% end %>
