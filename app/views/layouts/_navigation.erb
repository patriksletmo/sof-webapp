<div id="navgroup">
  <div id="slide-out" class="side-nav fixed">
    <ul>
      <li class="logo">
        <a href="/" class="brand-logo">
          <img src="https://s3-eu-west-1.amazonaws.com/lintek-sof/sof-homepage/logos/Logga_SOF_pafarg2.png">
        </a>
      </li>

      <% if current_user.nil? %>
          <li><%= link_to('Logga in', login_path, {:class => 'list-item waves-effect waves-teal'}) %></li>
      <% else %>
          <li class="no-padding">
            <ul class="collapsible collapsible-accordion">
              <li>
                <a class="collapsible-header">
                  Konto
                  <i class="material-icons indicator">arrow_drop_down</i>
                </a>
                <div class="collapsible-body">
                  <ul>
                    <li>
                      <a href="<%= profile_url %>">
                        Profil
                      </a>
                    </li>
                    <li>
                      <a href="<%= logout_url %>">
                        Logga ut
                      </a>
                    </li>
                  </ul>
                </div>
              </li>
            </ul>
          </li>
      <% end %>

      <% menu_items.each do |item| %>
          <% if item['menu_items'].any? %>
              <li class="no-padding">
                <ul class="collapsible collapsible-accordion">
                  <li>
                    <a class="collapsible-header menu-item">
                      <%= item['title'] %>
                      <i class="material-icons indicator">arrow_drop_down</i>
                    </a>
                    <div class="collapsible-body">
                      <ul>
                        <% item['menu_items'].each do |sub_menu_item| %>
                            <li>
                              <a href="#!" class="menu-item submenu-item">
                                <%= sub_menu_item['title'] %>
                              </a>
                            </li>
                        <% end %>
                      </ul>
                    </div>
                  </li>
                </ul>
              </li>
          <% else %>
              <li>
                <a href="#!" class="waves-effect waves-teal">
                  <i class="material-icons">cloud</i>
                  <%= item['title'] %>
                </a>
              </li>
          <% end %>
      <% end %>
    </ul>

    <ul class="lintek-logo-hearts">
      <li class="lintek-hearts">
        <%= link_to 'https://www.facebook.com/Studentorkesterfestivalen', :class => 'lintek-heart', target: :'_blank' do %>
            <img src="https://s3-eu-west-1.amazonaws.com/lintek-sof/webapp/lintek/larm_hjarta.png">
        <% end %>

        <%= link_to 'https://www.facebook.com/Studentorkesterfestivalen', :class => 'lintek-heart', target: :'_blank' do %>
            <img src="https://s3-eu-west-1.amazonaws.com/lintek-sof/webapp/lintek/klimatveckan_hjarta.png">
        <% end %>

        <%= link_to 'https://www.facebook.com/Studentorkesterfestivalen', :class => 'lintek-heart', target: :'_blank' do %>
            <img src="https://s3-eu-west-1.amazonaws.com/lintek-sof/webapp/lintek/lintek_hjarta.png">
        <% end %>

        <%= link_to 'https://www.facebook.com/Studentorkesterfestivalen', :class => 'lintek-heart', target: :'_blank' do %>
            <img src="https://s3-eu-west-1.amazonaws.com/lintek-sof/webapp/lintek/lithanian_hjarta.png">
        <% end %>

        <%= link_to 'https://www.facebook.com/Studentorkesterfestivalen', :class => 'lintek-heart', target: :'_blank' do %>
            <img src="https://s3-eu-west-1.amazonaws.com/lintek-sof/webapp/lintek/munchen_hjarta.png">
        <% end %>
      </li>
      <li class="lintek-logo-text-container">
        <span class="lintek-logo-text">Arrangeras av</span>
      </li>
      <li class="lintek-logo-container">
        <img src="https://s3-eu-west-1.amazonaws.com/lintek-sof/webapp/lintek/lintek_logga.png">
      </li>
    </ul>
  </div>
</div>